# Copy to .env for local development only.
# Do not commit real secrets.

# Mistral (required for embeddings, OCR, transcription, and generation)
MISTRAL_API_KEY=your_mistral_api_key
MISTRAL_BASE_URL=https://api.mistral.ai

# ElevenLabs (optional, for TTS/STT)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_BASE_URL=https://api.elevenlabs.io

# Server auth (optional, auto-generated by dir2mcp up if not set)
DIR2MCP_AUTH_TOKEN=

# CORS: additional allowed browser origins, comma-separated (optional)
DIR2MCP_ALLOWED_ORIGINS=

# x402 payment gating (optional)
DIR2MCP_X402_FACILITATOR_TOKEN=
